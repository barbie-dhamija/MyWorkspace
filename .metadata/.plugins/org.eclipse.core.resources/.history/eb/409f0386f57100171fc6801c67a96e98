		for(int i=1;i<l1;i++)
		{
			for(int k=i;k<=i;k++)
			{
				if(a1.get(i).equals(a2.get(k)))
				{
					System.out.println(a1.get(i)+ "  other is " +a2.get(k));
				}
				else
					System.out.println("not equal");
